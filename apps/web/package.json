{
    "name": "@audioling/web",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "build:desktop": "tauri build",
        "dev": "tauri dev",
        "dev:web": "vite",
        "generate:openapi": "bun run generate:openapi-json && bun run generate:openapi-client && pwsh -c ./orval-lint.ps1",
